{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist"
  },
  "package": {
    "productName": "watchcoin",
    "version": "0.0.2"
  },
  "tauri": {
    "systemTray": {
      "iconPath": "icons/icon.png",
      "iconAsTemplate": false
    },
    "allowlist": {
      "all": false,
      "fs": {
        "readFile": true,
        "scope": [
          "*",
          "$APP",
          "$APP/*",
          "$CACHE",
          "$CACHE/*",
          "$CONFIG",
          "$CONFIG/*",
          "$LOG",
          "$LOG/*",
          "$HOME/*"
        ]
      },
      "window": {
        "hide": true
      },
      "shell": {
        "all": false,
        "open": true
      }
    },
    "windows": [
      {
        "title": "setting",
        "width": 800,
        "height": 800
      }
    ],
    "security": {
      "csp": null
    },

    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.moyu.kline",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    }
  }
}
